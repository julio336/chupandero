<div class="navbar-wrapper">
      <!-- Wrap the .navbar in .container to center it within the absolutely positioned parent. -->
      <div class="container">

        <div class="navbar navbar-inverse">
          <div class="navbar-inner">
            <!-- Responsive Navbar Part 1: Button for triggering responsive navbar (not covered in tutorial). Include responsive CSS to utilize. -->
            <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
			<%= link_to image_tag('http://i1321.photobucket.com/albums/u543/Julio_Ahuactzin/logo_zps39cb8c6a.png', style: "width:140px"), root_path, class: "brand" %>
            <!-- Responsive Navbar Part 2: Place all navbar contents you want collapsed withing .navbar-collapse.collapse. -->
            <div class="nav-collapse collapse">
              <ul class="nav">
                <li><a href="#about"><i class="icon-glass icon-white"></i>Bares destacados</a></li>
                <li><a href="#contact"><i class="icon-info-sign icon-white"></i>Contacto</a></li>
                <!-- Read about Bootstrap dropdowns at http://twitter.github.com/bootstrap/javascript.html#dropdowns -->
              </ul>
			  <ul class="nav pull-right">
				<% if user_signed_in? %>
                 <li class="divider-vertical"></li>
                 <li class="dropdown">
                   <a href="#" class="dropdown-toggle" data-toggle="dropdown">Sesion de <strong><%= current_user.email %></strong> <b class="caret"></b></a>
                   <ul class="dropdown-menu">
                     <li><%= link_to "Editar", edit_user_registration_path %></li>
                     <li><%= link_to "Cerrar sesion", destroy_user_session_path, method: :delete %></li>
                   </ul>
                 </li>
				<% else %>
				<li><%= link_to "Registrarse", new_user_registration_path %></li>
				<li class="divider-vertical"></li>
                <li><%= link_to "Iniciar sesión", new_user_session_path %></li>
				<% end %>
			  </ul>
            </div><!--/.nav-collapse -->
          </div><!-- /.navbar-inner -->
        </div><!-- /.navbar -->

      </div> <!-- /.container -->
